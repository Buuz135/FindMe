{
  "schemaVersion": 1,
  "id": "findme",
  "version": "${version}",
  "name": "FindMe",
  "description": "FindMe allows you to search for items in nearby inventories by pressing a key.",
  "authors": [
    "Buuz135"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },
  "license": "MIT",
  "icon": "assets/examplemod/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.buuz135.findme.fabric.FindMeFabric"
    ],
    "client": [
      "com.buuz135.findme.fabric.FindMeClientFabric"
    ]
  },
  "mixins": [
    "findme-common.mixins.json"
  ],
  "accessWidener": "findme.accesswidener",
  "depends": {
    "fabricloader": ">=0.12.8",
    "fabric": "*",
    "minecraft": ">=1.20.1",
    "architectury": ">=1.1.55"
  },
  "suggests": {
    "flamingo": "*"
  }
}
